.noWrap {
	white-space: nowrap !important;
}

/* =========================== Change Mode =========================== */
#leftPanel {
	padding-left: 0;
	padding-right: 6px;
}

#rightPanel {
	padding-right: 0;
	padding-left: 6px;
}

.pendBadgeSpacer {
	margin-left: 6px;
}

#titleBar {
	font: bold 20px Arial;
	margin-bottom: 16px;
	color: #DB8100; /*INFO: white;*/

	min-height: 20px;
	padding: 16px 10px;
	background-color: #FFE6AB; /*INFO: #31b0d5;*/	/**/
	border: 1px solid #F2DCA7; /*INFO: #269abc;*/	/**/
	border-radius: 4px;
	-webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.05);
	box-shadow: inset 0 1px 1px rgba(0,0,0,.05);
}

#changeList {
	margin-top: 0;
	margin-left: 0;

	font-size: 14px;

	list-style-type: none;
	padding-left: 0;
}

.changeName {
	padding-bottom: 16px;
}

.changeItem {
	padding-bottom: 8px;
}

#changeList .vtp, #answerList .vtp {
	color: black;
}

.iconSpacer {
	margin-right: 6px;
	margin-left: -6px;
}

.iconSpacer>svg {
	vertical-align: middle;
	text-align: left;
}

.probTitle {
	margin-top: 2px;
}

.changePrefix {
	margin-bottom: 6px;
}

.changeProb {
	font-size: 14px;
}

.changeImg {
	margin-top: 10px;
	margin-bottom: 10px;
}

.changeImg>img {
	border: solid 3px black;
}

.imgOverlay {
	font: bold 12px Arial;
	position: absolute;
}

.changeChoices {
/*	margin-left: 10px; */
}

.changeOption {
	font-weight: normal;
}

.changeNoAnswer {
	font-weight: bold;
}

.answerSpacer {
	margin-top: 8px;
	margin-left: -10px;
}

.scoreBar {
	margin-bottom: 4px;
	margin-top: 8px;

	font: 14px Arial;
}

.textBold {
	font-weight: bold;
}

input.form-control {
	display: inline;

	padding: 0 4px;
	height: 26px;
}

form {
	display: inline;
}

.scoreBar input.gradeInp {
	width: 30px;
}

.scoreBarTitle {
	color: blue;
}

.scoreBarSpacer {
	margin-top: 14px;
}

.gradeMe {
	font-weight: bold;
}

.gradeMe.glyphicon {
	color: red;
}

.gradeMeSpacer {
	margin-right: 2px;
}

.pendingScore {
	background-color: pink;
	border: #C44 1px solid;
	border-radius: 4px;
	padding: 14px 10px;
}

/* =========================== Problem Types =========================== */

.mcCorrectAns {
	color: green;
}

.mcCorrectSub, .correctAns .mn {
	color: green;
}

.mcWrongSub, .incorrectAns .mn {
	color: red;
}

.mcSpacer {
	margin-bottom: 2px;
}


.graphConstTable {
	border-collapse:separate;
	border-spacing: 4px 8px;
}

.graphConstLabel {
	font: bold 15px Arial;
}

.graphConstValue {
	border: solid 1px black;
	padding: 2px;

	text-align: center;
}

/* =========================== Equal Height Panels =========================== */

.flexColWrapper {
	display: table;
	border-collapse:separate;
	border-spacing: 10px;
}

.flexColInner {
	display: table-row;
	border-spacing: 0;
}

.flexWidth {
	display: table-cell;
}

/* =========================== Graph route =========================== */
#graphContainer {
	padding-left:  0;
/*	padding-right: 0; */
}

#graphContainer .panel .list-group-item {
	cursor: pointer;

	-webkit-transition: background-color linear 0.3s, text-shadow linear 0.3s;
	transition: background-color linear 0.3s, text-shadow linear 0.3s;
}

#leftPanel>.panel {
	border-left: none;
	border-radius: 0 4px 4px 0;

	margin-bottom: 0;
}

#rightPanel>.panel {
/*
	border-right: none;
	border-radius: 4px 0 0 4px;
*/
}

#class_line {
	border-top: 1px solid #AAF;
}

accordion div.panel-heading {
	padding: 0;
}

#graphContainer .titlePad {
	padding: 10px 15px;
}

.shortBottom {
	margin-bottom: 0;
}

.hoverHighlight {
	cursor: pointer;

	-webkit-transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.3s;
	transition:all cubic-bezier(0.250, 0.460, 0.450, 0.940) 0.3s;
}

.hoverHighlight:hover {
	text-shadow: 0 0 10px #26f;
}

accordion .panel {
	margin-top: 0 !important;
	border-radius: 0 !important;
	border-top: none;
}



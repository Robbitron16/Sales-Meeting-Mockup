/* app css stylesheet */

/* TEMP! Highlight broken data */
span.math-outside {
	background-color: red;
}

input.ng-invalid {
/*	background-color: #F77E7E; */
	border-color: red;
/*	box-shadow: inset 0 1px 1px rgba(0,0,0,.075),0 0 8px rgba(102,0,0,.6); */
